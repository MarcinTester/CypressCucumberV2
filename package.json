{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules\\.bin\\cypress run",
    "headTest": "npm run test -- --headed",
    "chromeTest": "npm run test -- --browser chrome",
    "recordDashBoardTest": "npm run test -- --record --key c32b4dc7-deb8-4ad4-a130-5d9ffee0df92 --reporter mochawesome",
    "Booking-Regression": "npx cypress-tags run -e TAGS=\"@regression\" --headed --browser chrome",
    "Booking-InProgress": "npx cypress-tags run -e TAGS=\"@inProgress\" --headed --browser chrome",
    "Booking-Smoke": "npx cypress-tags run -e TAGS=\"@smoke\" --headed --browser chrome",
    "Booking-Regression-multi-browser": "npm run Booking-Regression -- --browser electron --headed & npm run Booking-Regression -- --browser chrome --headed",
    "Booking-InProgress-multi-browser": "npm run Booking-InProgress -- --browser electron --headed & npm run Booking-InProgress -- --browser chrome --headed"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/mochawesome": "^6.2.1",
    "cypress": "^9.5.4",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "multiple-cucumber-html-reporter": "^1.18.0"
  }
}
